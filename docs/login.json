{
  "paths": {
    "/login": {
      "post": {
        "tags": ["login"],
        "summary": "Login user with miniapp data",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "$ref": "./entities/appdata.json#/components/schemas/AppData/properties"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "User logined",
            "content": {
              "application/json": {
                "schema": {
                  "allOf": [
                    {
                      "$ref": "./generic/genericResponse.json#/components/schemas/GenericResponse"
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "./generic/genericResponse.json#/components/schemas/GenericResponse"
                }
              }
            }
          }
        }
      }
    }
  }
}
